    <style>
        .main-container {
          width: 100%;
          height: 100vh;
          max-height: 100vh;
          overflow: hidden;
          display: flex;
          justify-content: center;
          align-items: center;
          background-color: #61BFD4;
          padding: 90px 100px;
        }

        .text-container {
          width: 100%;
          box-sizing: border-box;
          margin-right: 1rem;
          background-color: white;
          border: 4px solid rgb(189, 189, 189);
          border-radius: 10px;
          height: 90vh;
          max-height: 90vh;
          overflow-y: scroll;
          overflow-x: hidden;
          color: rgb(80, 80, 80);
          display: flex;
          justify-content: flex-start;
          align-items: flex-start;
          flex-wrap: wrap;
          box-shadow:3px 5px 6px rgba(0,0,0,.4);
        }

        .input-chat {
          align-self: flex-end;
          box-sizing: content-box;
          position: absolute;
          width: 84.05%;
          background: white;
          border-radius: 5px;
        }

        #input {
          width: 94%;
          padding: 1rem;
          border: none;
          border: 2px solid #5984BA;
          color: rgb(0, 0, 0);
          font-size: 1.5rem;
          box-shadow: 1px 3px 5px rgba(0,0,0,.5);
        }

        hr.divider {
          padding: 0;
          margin: 1rem 0 1rem;
          border: none;
          border-bottom: 2px solid #e0e0e0;
        }

        ol#lista-users {
          margin-left: 1rem;
        }

        .mensajes-container {
          width: 100%;
          display: flex;
          justify-content: flex-start;
          align-items: flex-start;
          flex-wrap: wrap;
          margin-bottom: 60px;
        }

        .local-message {
          text-align: right;
          padding: 1rem;
          background-color: #7ED9ED;
          color: white;
          margin: 1rem;
          box-sizing: border-box;
          border-radius: 20px;
          width: 50%;
          margin-left: auto;
          margin-bottom: 0;
        }

        .remote-message {
          padding: 1rem;
          background-color: #5984BA;
          color: white;
          margin: 1rem;
          box-sizing: border-box;
          border-radius: 20px;
          width: 50%;
          margin-bottom: 0;
        }
        .imgmsg{
          height: 16px;
          width: 16px;
        }

    </style>
