                                    <div class="work-progres">
                                        <div class="table-responsive">
                                            <table class="table table-hover">
                                              <thead>
                                                <tr>
                                                  <th>#</th>
                                                  <th>Nombre</th>
                                                  <th>Paciente</th>

                                                  <th>Estado</th>
                                                  <th>Avance</th>

                                                  <th> Más</th>
                                              </tr>
                                          </thead>
                                          <tbody>
                                            <tr>
                                              <td>1</td>
                                              <td>Tratamiento: Enfermedad coronaria</td>
                                              <td>Paciente 1</td>

                                              <td><span class="label label-danger">En progreso</span></td>
                                              <td><span class="badge badge-info">50%</span></td>
                                              <td><a href="cita"><i class="fa fa-plus"></i></a></td>
                                          </tr>
                                          <tr>
                                              <td>2</td>
                                              <td>Tratamiento: Enfermedad coronaria</td>
                                              <td>Paciente 2</td>

                                              <td><span class="label label-success">Terminado</span></td>
                                              <td><span class="badge badge-success">100%</span></td>
                                              <td><a href="cita"><i class="fa fa-plus"></i></a></td>
                                          </tr>
                                          <tr>
                                              <td>3</td>
                                              <td>Tratamiento: Enfermedad coronaria</td>
                                              <td>Paciente 3</td>

                                              <td><span class="label label-warning">En progreso</span></td>
                                              <td><span class="badge badge-warning">80%</span></td>
                                              <td><a href="cita"><i class="fa fa-plus"></i></a></td>
                                          </tr>

                                      </tbody>
                                  </table>
                                </div>
                            </div>
                    </div>
                     <div class="clearfix"></div>

                </div>

               <div class="agile-tables" style="margin-top: 30px">
                    <div class="w3l-table-info agile_info_shadow">
                        <div class="container">
                            <div class="row">
                               <div class="col-md-6">
                                   <h3 class="w3_inner_tittle two">Historico de gastos</h3>
                                    <canvas id="higa"></canvas>
                               </div>
                               <div class="col-md-1"></div>
                                <div class="col-md-5">
                                <h3 style="margin-bottom: 30px">Ingresos y egresos</h3>
                                    <div class="table-responsive-lg">
                                        <table class="table table-hover table-striped" >
                                          <thead class="thead-dark" style="background-color: #FF5E61">
                                            <tr>
                                              <th scope="col">Nombre</th>
                                              <th scope="col">Fecha</th>
                                              <th scope="col">Valor</th>
                                              <th scope="col">Tipo</th>
                                              <th scope="col">Descripcion</th>
                                            </tr>
                                          </thead>
                                          <tbody>
                                            <tr>
                                              <th scope="row">Material</th>
                                              <td>dd/mm/yyyy</td>
                                              <td>$25</td>
                                                <td><span class="label label-danger">Gasto</span></td>
                                              <td><a href="cita"><i class="fa fa-plus"> Ver mas </i></a></td>
                                            </tr>
                                            <tr>
                                              <th scope="row">Cita</th>
                                              <td>dd/mm/yyyy</td>
                                              <td>$25</td>
                                                <td><span class="label label-success">Ingreso</span></td>
                                              <td><a href="cita"><i class="fa fa-plus"> Ver mas </i></a></td>
                                            </tr>
                                            <tr>
                                              <th scope="row">Totales</th>
                                              <td></td>
                                              <td>$25</td>
                                                <td></td>
                                                <td></td>
                                            </tr>

                                          </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>

                </div>
        </div>
    </div>
