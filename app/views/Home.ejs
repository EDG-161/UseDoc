<script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>
<script>
    var ctx = document.getElementById('hipa').getContext('2d');
    var myLineChart = new Chart(ctx, {
        type: 'line',
        data: {
            labels: ['mm/yy', 'mm2/yy', 'mm3/yy', 'mm4/yy', 'mm5/yy', 'mm6yy', 'mm7/yy'],
            datasets: [{
                label: 'Pacientes',
                backgroundColor: '#87CCA7',
                borderColor: '#87CCA7',
                data: [0, 10, 5, 2, 20, 30, 45]
            }]
        },
        options: {}
    });
    var ctx = document.getElementById('higa').getContext('2d');
    var myLineChart = new Chart(ctx, {
        type: 'line',
        data: {
            labels: ['mm/yy', 'mm2/yy', 'mm3/yy', 'mm4/yy', 'mm5/yy', 'mm6yy', 'mm7/yy'],
            datasets: [{
                label: 'Ingrsos',
                backgroundColor: '#FF5E61',
                borderColor: '#FF5E61',
                data: [500, 550, 490, 300, 450, 200, 500],
                "fill":false,
                "lineTension":0.1
            }]
        },
        options: {}
    });
</script>
